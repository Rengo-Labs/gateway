{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "allowedSlippage": { "type": "string" },
    "gasLimitEstimate": { "type": "integer" },
    "ttl": { "type": "integer" },
    "nativeCurrencySymbol": { "type": "string" },
    "timeToLive": { "type": "integer" },
    "contractAddresses": {
      "type": "object",
      "patternProperties": {
        "^[\\w-]+$": {
          "type": "object",
          "properties": {
            "routerAddress": { "type": "string" },
            "nodeURL": { "type": "string" },
            "tokenURL": { "type": "string" }
          },
          "required": ["routerAddress", "nodeURL", "tokenURL"],
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false,
  "required": [
    "allowedSlippage",
    "gasLimitEstimate",
    "ttl",
    "nativeCurrencySymbol",
    "timeToLive",
    "contractAddresses"
  ]
}
